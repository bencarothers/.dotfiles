alias e='$EDITOR'
alias airport='/System/Library/PrivateFrameworks/Apple80211.framework/Versions/Current/Resources/airport'

# Web Security
alias intercept='sudo networksetup -setwebproxystate wi-fi on'
alias nocept='sudo networksetup -setwebproxystate wi-fi off'
alias webgoat='java -jar ~/Documents/web\ security/WebGoat.jar'
alias burp='java -jar ~/Documents/web\ security/burp.jar'

# vimrc editing
alias ve='vim ~/.vimrc'
alias vb='vim ~/.vimrc.bundles'

# zsh profile editing
alias ze='vim ~/.zshrc'
alias zr='source ~/.zshrc'
alias reload!='. ~/.zshrc'

